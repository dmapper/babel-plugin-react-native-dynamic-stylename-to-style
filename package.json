{
  "name": "babel-plugin-react-native-dynamic-stylename-to-style",
  "version": "0.14.0",
  "description": "Dynamically resolve styleName in RN with support for multi-class selectors (for easier modifiers)",
  "keywords": [
    "babel",
    "babel-plugin",
    "react-native",
    "stylename",
    "style"
  ],
  "main": "index.js",
  "scripts": {
    "release": "npmpub",
    "test": "jest --coverage",
    "lint": "eslint ."
  },
  "author": "Pavel Zhukov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmapper/babel-plugin-react-native-dynamic-stylename-to-style"
  },
  "files": [
    "CHANGELOG.md",
    "process.js",
    "index.js",
    "README.md"
  ],
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-plugin-tester": "^5.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.12.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "lint-staged": "^8.1.0",
    "npmpub": "^4.1.0",
    "prettier": "^1.16.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "jest": true
    },
    "rules": {
      "space-before-function-paren": [
        2,
        "never"
      ],
      "semi": [
        2,
        "always"
      ],
      "quotes": 0,
      "no-unused-vars": 2
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none"
  },
  "jest": {
    "coverageDirectory": "./.coverage/",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  },
  "dependencies": {
    "react-native-dynamic-style-processor": "^0.21.0"
  }
}
